
public class inter {
	public static void main(String[] args) throws Exception{
		String Id;
		int tal;
		String rest;
		int index;
		for(String x : args){
			rest= x.toLowerCase();
			
			if(args[0].contains("som"))
			{
				index = rest.indexOf("som");
				Id = rest.substring(0, index);
				rest = rest.substring(index);
				
				index = rest.indexOf("=");
				tal=Integer.parseInt(rest.substring(index + 1));
				
				System.out.print("int " + Id + "=" + tal);
			}
			System.out.print("nope");
				
		}
		
		
	}
}
