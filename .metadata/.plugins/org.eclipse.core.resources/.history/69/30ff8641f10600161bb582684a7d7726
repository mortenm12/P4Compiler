package AST;

public class Dcl_Var implements IASTNode {
	Dcl_Var node;
	
	public String ID;

	
	@Override
	public String CodeGenration() {
		// TODO Auto-generated method stub
		return null;
	}
	
	public void SymbolTableCode() {
	}
	
	class Dcl extends Dcl_Var{
		
		Type Type;
	}
	
	class Ass extends Dcl_Var{
		Operation Value;
	}
	
	class Dcl_Ass extends Dcl_Var{
		String Type;
		Operation Value;
	}
	
	class List_Ass extends Dcl_Var{
		Operation Index;
		Operation Value;
	}
	
	class List_Add extends Dcl_Var{
		Operation Value;
		
	}
	
	class List_Del extends Dcl_Var{
		Operation Index;
	}

}
